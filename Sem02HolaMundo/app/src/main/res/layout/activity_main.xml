<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sexo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#cfd5e5"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/r_contraeña"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:background="@color/white"
        android:ems="10"
        android:inputType="textPassword"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/R_email"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/R_email"
        app:layout_constraintTop_toBottomOf="@+id/R_email"
        tools:text="@string/R_contraseña" />

    <TextView
        android:id="@+id/Fondo_1"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:background="#3e579e"
        android:text="@string/I_contraeña"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/fondo_1" />

    <TextView
        android:id="@+id/txt_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        android:text="@string/txt_1"
        android:textColor="@color/white"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/I_correo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@android:color/background_light"
        android:drawableLeft="@drawable/cuadrado_svg"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/txt_1"
        app:layout_constraintTop_toBottomOf="@+id/txt_1"
        tools:text="@string/I_correo" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="100sp"
        android:layout_height="15sp"
        android:layout_marginTop="5dp"
        android:background="#3e579e"
        android:bufferType="editable"
        android:button="@drawable/fb_icon_325x325"
        android:isScrollContainer="true"
        android:text="@string/check_nocerrar"
        android:textColor="@android:color/background_light"
        android:textSize="10sp"
        app:buttonTint="@color/white"
        app:layout_constraintStart_toStartOf="@+id/I_correo"
        app:layout_constraintTop_toBottomOf="@+id/I_correo" />

    <TextView
        android:id="@+id/txt_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="@string/txt_contraseña"
        android:textColor="@android:color/background_light"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/txt_1"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/txt_1" />

    <EditText
        android:id="@+id/I_contraseña"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@android:color/background_light"
        android:drawableLeft="@drawable/llave"
        android:ems="10"
        android:inputType="textPassword"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/txt_2"
        app:layout_constraintTop_toBottomOf="@+id/txt_2"
        tools:text="@string/I_contraeña" />

    <TextView
        android:id="@+id/txt_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/txt_3"
        android:textColor="#7d9bd5"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/I_contraseña"
        app:layout_constraintTop_toBottomOf="@+id/I_contraseña" />

    <Button
        android:id="@+id/btn_1"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginStart="24dp"
        android:autoText="true"
        android:drawableLeft="@drawable/select_icon_151113"
        android:text="@string/btn_1"
        android:textAlignment="center"
        android:textSize="8sp"
        app:backgroundTint="#647592"
        app:layout_constraintBottom_toBottomOf="@+id/I_contraseña"
        app:layout_constraintStart_toEndOf="@+id/I_contraseña"
        app:layout_constraintTop_toTopOf="@+id/I_contraseña" />

    <TextView
        android:id="@+id/txt_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:drawableRight="@drawable/fb_icon_325x325"
        android:fontFamily="sans-serif-black"
        android:text="@string/txt_4"
        android:textColor="#2a426f"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Fondo_1" />

    <TextView
        android:id="@+id/txt_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/txt_5"
        android:textColor="#384661"
        app:layout_constraintStart_toStartOf="@+id/txt_4"
        app:layout_constraintTop_toBottomOf="@+id/txt_4" />

    <TextView
        android:id="@+id/txt_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/txt_6"
        android:textAlignment="viewEnd"
        android:textColor="#25355d"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/txt_5" />

    <TextView
        android:id="@+id/txt_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/txt_7"
        android:textAlignment="viewEnd"
        android:textColor="#25355d"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/txt_6" />

    <TextView
        android:id="@+id/txt_8"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/txt_8"
        android:textAlignment="viewEnd"
        android:textColor="#25355d"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/txt_7" />

    <TextView
        android:id="@+id/txt_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/txt_9"
        android:textAlignment="viewEnd"
        android:textColor="#25355d"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/txt_8" />

    <TextView
        android:id="@+id/txt_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/txt_10"
        android:textAlignment="viewEnd"
        android:textColor="#25355d"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/txt_9" />

    <TextView
        android:id="@+id/txt_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/txt_11"
        android:textAlignment="viewEnd"
        android:textColor="#25355d"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/txt_10" />

    <TextView
        android:id="@+id/txt_12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        android:drawableLeft="@drawable/busqueda"
        android:text="@string/txt_12"
        android:textColor="#25355d"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_registrar"
        app:layout_constraintEnd_toEndOf="@+id/editTextDate3"
        app:layout_constraintStart_toStartOf="@+id/editTextDate2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="131dp" />

    <EditText
        android:id="@+id/R_nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/cuadrado_svg"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/R_nombre"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/txt_6"
        app:layout_constraintTop_toBottomOf="@+id/txt_5" />

    <EditText
        android:id="@+id/R_apellidos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/cuadrado_svg"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/R_apellidos"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/R_nombre"
        app:layout_constraintStart_toStartOf="@+id/R_nombre"
        app:layout_constraintTop_toBottomOf="@+id/R_nombre" />

    <EditText
        android:id="@+id/R_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/busqueda"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="@string/R_email"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/R_apellidos"
        app:layout_constraintStart_toStartOf="@+id/R_apellidos"
        app:layout_constraintTop_toBottomOf="@+id/R_apellidos" />

    <EditText
        android:id="@+id/R_contraeña"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/llave"
        android:ems="10"
        android:inputType="textPassword"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/R_email"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/R_email"
        app:layout_constraintTop_toBottomOf="@+id/R_email"
        tools:text="@string/R_contraseña" />

    <Button
        android:id="@+id/btn_registrar"
        android:layout_width="178dp"
        android:layout_height="30dp"
        android:layout_marginBottom="80dp"
        android:drawableLeft="@drawable/fb_icon_325x325"
        android:text="@string/registrarse"
        android:textSize="10sp"
        app:backgroundTint="#72ae59"
        app:layout_constraintBottom_toTopOf="@+id/txt_15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txt_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        android:text="@string/txt_13"
        android:textColor="#54629b"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/txt_15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="100dp"
        android:shadowColor="#929aa8"
        android:text="@string/txt_15"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="537dp" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:layout_marginStart="5dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/select_icon_151113"
        android:ems="10"
        android:hint="mes"
        android:inputType="date"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextDate2"
        app:layout_constraintStart_toEndOf="@+id/editTextDate2"
        app:layout_constraintTop_toTopOf="@+id/editTextDate2" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/select_icon_151113"
        android:ems="10"
        android:hint="dia"
        android:inputType="date"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/txt_11"
        app:layout_constraintStart_toEndOf="@+id/txt_11"
        app:layout_constraintTop_toTopOf="@+id/txt_11" />

    <EditText
        android:id="@+id/editTextDate3"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:layout_marginStart="5dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/select_icon_151113"
        android:ems="10"
        android:hint="año"
        android:inputType="date"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextDate"
        app:layout_constraintStart_toEndOf="@+id/editTextDate"
        app:layout_constraintTop_toTopOf="@+id/editTextDate" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="50dp"
        android:layout_height="15dp"
        android:layout_marginStart="20dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/select_icon_151113"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/txr_123"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/txt_10"
        app:layout_constraintStart_toEndOf="@+id/txt_10"
        app:layout_constraintTop_toTopOf="@+id/txt_10" />

</androidx.constraintlayout.widget.ConstraintLayout>